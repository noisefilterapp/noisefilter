{
  "name": "noise-filter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "web-ext lint --source-dir dist",
    "debug-run": "web-ext run --source-dir dist --firefox=/Applications/'Firefox Developer Edition.app'/Contents/MacOS/firefox-bin --start-url about:debugging#/runtime/this-firefox",
    "ext-run": "web-ext run --source-dir dist --firefox=/Applications/'Firefox Developer Edition.app'/Contents/MacOS/firefox-bin",
    "debug": "gulp debug && npm run lint",
    "build": "gulp build",
    "devstart": "npm run debug && npm run debug-run",
    "start": "npm run build && npm run ext-run"
  },
  "dependencies": {},
  "devDependencies": {
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-cache": "^1.1.3",
    "gulp-clean-css": "^4.2.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-jsonminify": "^1.1.0",
    "gulp-sass": "^4.0.2",
    "gulp-terser": "^1.2.0",
    "web-ext": "^4.0.0",
    "webextension-polyfill": "^0.6.0"
  }
}
